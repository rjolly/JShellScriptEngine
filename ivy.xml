<?xml version="1.0" encoding="UTF-8"?>
<ivy-module version="2.0" xmlns:e="http://ant.apache.org/ivy/extra">
    <info module="jshellscriptengine" organisation="com.github.rjolly" revision="1.0">
        <description homepage="http://github.com/dmac100/JShellScriptEngine/"/>
    </info>
    <configurations>
        <conf name="default" extends="runtime,master"/>
        <conf name="master"/>
        <conf name="compile"/>
        <conf name="runtime"/>
        <conf name="compile-test"/>
        <conf name="runtime-test"/>
        <conf name="sources"/>
        <conf name="javadoc"/>
        <conf name="pom"/>
    </configurations>
    <publications>
        <artifact name="jshellscriptengine" type="jar" ext="jar" conf="master"/>
        <artifact name="jshellscriptengine" type="source" ext="jar" conf="sources" e:classifier="sources"/>
        <artifact name="jshellscriptengine" type="javadoc" ext="jar" conf="javadoc" e:classifier="javadoc"/>
        <artifact name="jshellscriptengine" type="pom" ext="pom" conf="pom"/>
    </publications>
</ivy-module>
